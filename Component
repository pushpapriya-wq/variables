APP.JS:
import React from 'react';

const Fruitlist=()=>{
  const fruits=['apple','banana','orange'];
  return(
    <div>
      <h1>fruits list</h1>
      <ul>
        {fruits.map((fruits,index)=>(
          <li key={index}>{fruits}</li>
        ))}
      </ul>
    </div>
  )
}
export default Fruitlist;
